<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="513.0" prefWidth="468.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@/css/design.css" />
    </stylesheets>
    <children>
        <Label alignment="CENTER" layoutX="3.0" layoutY="41.0" prefHeight="57.0" prefWidth="249.0" text="You have chosen this car">
            <font>
                <Font size="19.0" />
            </font>
        </Label>
        <TextField fx:id="vehicleInfoField" editable="false" layoutX="252.0" layoutY="53.0" prefHeight="34.0" prefWidth="198.0" />
        <Label alignment="CENTER" layoutX="103.0" layoutY="117.0" prefHeight="34.0" prefWidth="296.0" text="Please enter reservation information">
            <font>
                <Font name="Avenir Next Condensed Bold" size="19.0" />
            </font>
        </Label>
        <DatePicker fx:id="pickupDate" editable="false" layoutX="205.0" layoutY="177.0" />
        <Label alignment="CENTER" layoutX="73.0" layoutY="177.0" prefHeight="27.0" prefWidth="108.0" text="Pickup date">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label alignment="CENTER" layoutX="68.0" layoutY="227.0" prefHeight="34.0" prefWidth="118.0" text="Return date">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <DatePicker fx:id="returnDate" editable="false" layoutX="205.0" layoutY="231.0" />
        <Label alignment="CENTER" layoutX="45.0" layoutY="290.0" prefHeight="34.0" prefWidth="164.0" text="First Name">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <TextField fx:id="firstNameField" layoutX="209.0" layoutY="294.0" prefHeight="27.0" prefWidth="220.0" />
        <Label alignment="CENTER" layoutX="91.0" layoutY="385.0" prefHeight="27.0" prefWidth="74.0" text="Email">
         <font>
            <Font size="14.0" />
         </font></Label>
        <TextField fx:id="emailField" layoutX="209.0" layoutY="385.0" prefHeight="27.0" prefWidth="220.0" />
        <Button fx:id="finishBtn" layoutX="165.0" layoutY="437.0" mnemonicParsing="false" onAction="#onFinish" prefHeight="49.0" prefWidth="126.0" text="Finish">
            <font>
                <Font name="Georgia" size="17.0" />
            </font>
        </Button>
      <Label alignment="CENTER" layoutX="51.0" layoutY="332.0" prefHeight="42.0" prefWidth="152.0" text="Last Name">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="lastNameField" layoutX="209.0" layoutY="339.0" prefHeight="27.0" prefWidth="220.0" />
    </children>

</AnchorPane>
